{
  "hosting": {
    "public": "out",
    "ignore": ["**/.*", "**/node_modules/**"],
    "rewrites": [
      {
        "destination": "/info/[slug].html", 
        "regex": "^/info/([^/]+?)(?:/)?$"
      },
      {
        "destination": "/managementInfo/[slug].html", 
        "regex": "^/managementInfo/([^/]+?)(?:/)?$"
      }
    ]
  }
}
